{
    "classes": [
        "::oaeservice::etherpad"
    ],
    
    "rsyslog::imfiles": [
        {
            "path": "/var/log/etherpad/access.log",
            "tag": "access",
            "state_file_name": "etherpad_access",
            "severity": "info",
            "facility": "local0",
            "poll_interval_seconds": 10
        },
        {
            "path": "/var/log/etherpad/error.log",
            "tag": "error",
            "state_file_name": "nginx_error",
            "severity": "error",
            "facility": "local1",
            "poll_interval_seconds": 10
        }
    ],


    "nagios_hostgroup": "misc"
}